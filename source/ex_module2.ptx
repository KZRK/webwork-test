    <webwork>
      <pg-code>
        $a = random(1,3,1);
        $b = random(4,6,1);
        $c = random(1,3,1);
        $d = random(4,6,1);

        Context("Fraction");
        $ans1 = Fraction($a*$c, $b*$d);
      </pg-code>
      <statement>
        <p>The expression</p>
        <p><m>\left(y^{\frac{<var name="$a"/>}{<var name="$b"/>}}\right)^{\frac{<var name="$c"/>}{<var name="$d"/>}}</m></p>
        <p>equals <m>y^r</m> where <m>r</m>, the exponent of <m>y</m>, is:</p>
        <p><var name="$ans1" width="5"/></p>
      </statement>
